namespace BFGame;

table Texture {
    id:         uint (key);
    debug_name: string;
    atlas_x:    uint;
    atlas_y:    uint;
    size_x:     uint;
    size_y:     uint;
}

table Atlas {
    size_x:   uint;
    size_y:   uint;
    textures: [Texture];
}

file_identifier "ATLS";
root_type Atlas;
